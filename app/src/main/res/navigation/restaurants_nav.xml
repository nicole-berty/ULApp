<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/listOfRestaurants">

    <fragment
        android:id="@+id/listOfRestaurants"
        android:name="ulapp.myUL.restaurant_menus.ListOfRestaurants"
        android:label="fragment_list_of_restaurants"
        tools:layout="@layout/fragment_list_of_restaurants" >
        <action
            android:id="@+id/action_listOfRestaurants_to_stablesClubMenu"
            app:destination="@id/stablesClubMenu" />
        <action
            android:id="@+id/action_listOfRestaurants_to_scholarsMenu"
            app:destination="@id/scholarsMenu" />
        <action
            android:id="@+id/action_listOfRestaurants_to_paddocksMenu"
            app:destination="@id/paddocksMenu" />
        <action
            android:id="@+id/action_listOfRestaurants_to_pavilionsMenu"
            app:destination="@id/pavilionsMenu" />
        <action
            android:id="@+id/action_listOfRestaurants_to_sportsBarMenu"
            app:destination="@id/sportsBarMenu" />
    </fragment>
    <fragment
        android:id="@+id/stablesClubMenu"
        android:name="ulapp.myUL.restaurant_menus.StablesClubMenu"
        android:label="fragment_stables_club_menu"
        tools:layout="@layout/fragment_stables_club_menu" />
    <fragment
        android:id="@+id/scholarsMenu"
        android:name="ulapp.myUL.restaurant_menus.ScholarsMenu"
        android:label="fragment_scholars_menu"
        tools:layout="@layout/fragment_scholars_menu" />
    <fragment
        android:id="@+id/paddocksMenu"
        android:name="ulapp.myUL.restaurant_menus.PaddocksMenu"
        android:label="fragment_paddocks_menu"
        tools:layout="@layout/fragment_paddocks_menu" />
    <fragment
        android:id="@+id/pavilionsMenu"
        android:name="ulapp.myUL.restaurant_menus.PavilionsMenu"
        android:label="fragment_pavilions_menu"
        tools:layout="@layout/fragment_pavilions_menu" />
    <fragment
        android:id="@+id/sportsBarMenu"
        android:name="ulapp.myUL.restaurant_menus.SportsBarMenu"
        android:label="fragment_sports_bar_menu"
        tools:layout="@layout/fragment_sports_bar_menu" />
</navigation>